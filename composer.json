{
    "name": "mockfromyaml/mockfromyaml",
    "description": "PHP traits to mock objects from arrays and read the data provider arrays from yaml files.",
    "type": "library",
    "require": {
        "phpunit/phpunit": ">=7",
        "symfony/yaml": ">=4.0"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "mcn-fredw",
            "email": "fred.woods.se@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:mcn-fredw/mock-from-yaml-php.git"
        }
    ],
    "autoload": {
        "psr-4": {"MockFromYaml\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "MockFromYaml\\Tests\\": "Tests/",
            "MockFromYaml\\ExampleProject\\": "ExampleProject/"
        }
    }
}
